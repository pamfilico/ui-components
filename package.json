{
  "name": "@pamfilico/uicomponents",
  "version": "0.1.6",
  "description": "Reusable UI components for Material UI and other frameworks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./material/auth": {
      "types": "./dist/material/auth/index.d.ts",
      "import": "./dist/material/auth/index.js",
      "require": "./dist/material/auth/index.js"
    },
    "./material/header": {
      "types": "./dist/material/header/index.d.ts",
      "import": "./dist/material/header/index.js",
      "require": "./dist/material/header/index.js"
    },
    "./material/header/homepage": {
      "types": "./dist/material/header/homepage/index.d.ts",
      "import": "./dist/material/header/homepage/index.js",
      "require": "./dist/material/header/homepage/index.js"
    },
    "./material/home": {
      "types": "./dist/material/home/index.d.ts",
      "import": "./dist/material/home/index.js",
      "require": "./dist/material/home/index.js"
    },
    "./material/hero": {
      "types": "./dist/material/hero/index.d.ts",
      "import": "./dist/material/hero/index.js",
      "require": "./dist/material/hero/index.js"
    },
    "./material/music": {
      "types": "./dist/material/music/index.d.ts",
      "import": "./dist/material/music/index.js",
      "require": "./dist/material/music/index.js"
    },
    "./material/music/utils": {
      "types": "./dist/material/music/utils.d.ts",
      "import": "./dist/material/music/utils.js",
      "require": "./dist/material/music/utils.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "release:patch": "npm version patch && npm run build && npm publish --access public",
    "release:minor": "npm version minor && npm run build && npm publish --access public",
    "release:major": "npm version major && npm run build && npm publish --access public",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build && node .storybook/fix-github-pages.js",
    "deploy-storybook": "npm run build-storybook && gh-pages -d storybook-static -b gh-pages --dotfiles"
  },
  "keywords": [
    "ui",
    "components",
    "material-ui",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "next": "^15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/nextjs": "^9.1.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "gh-pages": "^6.3.0",
    "msw": "^2.11.3",
    "msw-storybook-addon": "^2.0.5",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.4.1",
    "storybook": "^9.1.10",
    "typescript": "5.7.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "dependencies": {
    "@blocknote/core": "^0.40.0",
    "@blocknote/mantine": "^0.40.0",
    "@blocknote/react": "^0.40.0",
    "@mantine/core": "^7.17.8",
    "@mantine/hooks": "^7.17.8",
    "@tiptap/extensions": "^3.6.6",
    "react-markdown": "^10.1.0"
  }
}
